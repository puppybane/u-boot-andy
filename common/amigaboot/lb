setenv loadbmps run lb\; run lp-part2
setenv lb run lb1 lb2 lb3 lb4 lb5 lb6 lb7 lb8 lb9 lb10 lb11
setenv lp-part2 run lb12 lb13 lb14 lb15
setenv lb1 mmc read 0x10001000 0x800 0x100\;mmc read 0x10002000 0x1000 0x100\;mmc read 0x10004000 0x1010 0x100
setenv lb2 mmc read 0x10006000 0x1020 0x100\;mmc read 0x10008000 0x1030 0x100\;mmc read 0x1000a000 0x1040 0x100
setenv lb3 mmc read 0x1000c000 0x1050 0x100\;mmc read 0x1000e000 0x1060 0x100\;mmc read 0x10010000 0x1070 0x100
setenv lb4 mmc read 0x10012000 0x1080 0x100\;mmc read 0x10014000 0x1090 0x100\;mmc read 0x10016000 0x10a0 0x100
setenv lb5 mmc read 0x10018000 0x10b0 0x100\;mmc read 0x1001a000 0x10c0 0x100\;mmc read 0x1001c000 0x10d0 0x100
setenv lb6 mmc read 0x1001e000 0x10e0 0x100\;mmc read 0x10020000 0x10f0 0x100\;mmc read 0x10022000 0x1100 0x100
setenv lb7 mmc read 0x10024000 0x1110 0x100\;mmc read 0x10026000 0x1120 0x100\;mmc read 0x10028000 0x1130 0x100
setenv lb8 mmc read 0x1002a000 0x1140 0x100\;mmc read 0x1002c000 0x1150 0x100\;mmc read 0x1002e000 0x1160 0x100
setenv lb9 mmc read 0x10030000 0x1170 0x100\;mmc read 0x10032000 0x1180 0x100\;mmc read 0x10034000 0x1190 0x100
setenv lb10 mmc read 0x10036000 0x11a0 0x100\;mmc read 0x10038000 0x11b0 0x100\;mmc read 0x10003a000 0x11c0 0x100
setenv lb11 mmc read 0x11000000 0x2000 0x1000\;mmc read 0x11800000 0x4000 0x1000
setenv lb12 mmc read 0x12000000 0x8000 0x8000 \;mmc read 0x13000000 0x10000 0x8000\;mmc read 0x14000000 0x18000 0x8000
setenv lb13 mmc read 0x15000000 0x20000 0x8000 \;mmc read 0x16000000 0x28000 0x8000\;mmc read 0x17000000 0x30000 0x8000
setenv lb14 mmc read 0x18000000 0x38000 0x8000 \;mmc read 0x19000000 0x40000 0x8000\;mmc read 0x1a000000 0x48000 0x8000
setenv lb15 mmc read 0x1b000000 0x50000 0x8000 \;mmc read 0x1c000000 0x58000 0x8000\;mmc read 0x1d000000 0x60000 0x8000
saveenv
run loadbmps
